 <script>
    //exemplo 1
    const numeros = [1,2,3,4,5];

    const todos_positivos = numeros.every(numero => numero > 0);

    console.log(todos_positivos);
    //saida: true

    //exemplo 2
    const nomes = ['ana', 'carlos', 'beatriz'];

    const mais_de_tres_letras = nomes.every(nome => nome.lenght > 3)

    console.log(mais_de_tres_letras);
    //saida: false

    //exemplo 3
    const alunos = [
        {nome: 'joao', aprovado: true},
        {nome: 'maria', aprovado: true},
        {nome: 'julia', aprovado: true}
    ];

    const todos_aprovados = alunos.every(aluno => aluno.aprovado);

    console.log(todos_aprovados);
    //saida: true

</script>
